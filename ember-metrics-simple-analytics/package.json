{
  "name": "ember-metrics-simple-analytics",
  "version": "1.0.0",
  "description": "ember-metrics adapter for Simple Analytics.",
  "keywords": [
    "ember-addon",
    "ember-metrics",
    "metrics",
    "analytics",
    "simple-analytics",
    "simpleanalytics",
    "tracking",
    "gdpr"
  ],
  "repository": "https://github.com/mrloop/ember-metrics-simple-analytics",
  "license": "MIT",
  "author": "Ewan McDougall <ewan@mrloop.com>",
  "files": [
    "addon-main.js",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "lint": "concurrently 'npm:lint:*(!fix)' --names 'lint:'",
    "lint:fix": "concurrently 'npm:lint:*:fix' --names 'fix:'",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js:fix": "eslint . --fix",
    "start": "rollup --config --watch",
    "test": "echo 'A v2 addon does not have tests, run tests in test-app'",
    "prepack": "rollup --config"
  },
  "dependencies": {
    "@embroider/addon-shim": "1.8.7",
    "@ember/string": "3.1.1",
    "ember-metrics": "2.0.0-beta.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.0",
    "@babel/plugin-syntax-decorators": "^7.17.0",
    "@embroider/addon-dev": "4.3.1",
    "@rollup/plugin-babel": "^6.0.0",
    "concurrently": "^8.0.0",
    "ember-template-lint": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-ember": "^12.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-copy": "^3.4.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.js",
    "app-js": {}
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./addon-main.js": "./addon-main.js"
  }
}
